import java.util.Scanner;

public class GreatestOfThree {
    public static void main(String[] args) {
        int x,y,z;
        int max;
        Scanner sc = new Scanner(System.in);
        x = sc.nextInt();
        y = sc.nextInt();
        z = sc.nextInt();
        if( x>y && x>z){
            max = x;

        } else if (y>x && y>z) {
            max = y;

        }
        else
            max=z;
        System.out.println(max);

    }
}
